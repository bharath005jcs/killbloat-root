<?xml version="1.0" encoding="utf-8"?>
<manifest android:versionCode="18" android:versionName="1.3.4" package="com.tvkdevelopment.nobloat"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_SUPERUSER" />
    <application android:label="@string/app_name" android:icon="@drawable/icon">
        <activity android:label="@string/app_name" android:name=".activities.Main" android:configChanges="keyboardHidden|orientation">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.GeneralSettings" android:configChanges="keyboardHidden|orientation" />
        <activity android:name=".activities.SystemApps" android:configChanges="keyboardHidden|orientation" />
        <activity android:name=".activities.Blacklist" android:configChanges="keyboardHidden|orientation" />
        <activity android:name=".activities.DisabledApps" android:configChanges="keyboardHidden|orientation" />
        <activity android:name=".activities.BackedUpApps" android:configChanges="keyboardHidden|orientation" />
    </application>
</manifest>